#include <bits/stdc++.h>
using namespace std;
int main()
{
int i, j ,k ,n, count=0;
cin >> n >> k;
int array[n];
for(i = 0; i < n ; i++)
{
cin >> array[i];
}
for(i = 0; i < n ; i++)
{
for(j = i+1; j < n ; j++)
{
//if(i < j)
//{
if((array[i] + array[j]) % k == 0)
count++;
//}
}
}
cout<<count;

return 0;

}
